<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<HBox prefHeight="400.0" prefWidth="320.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="week3.Lab3Controller">
    <TextArea fx:id="textArea" prefHeight="200.0" prefWidth="200.0"/>
    <VBox prefHeight="200.0" prefWidth="100.0">
        <Button mnemonicParsing="false" onAction="#open" text="Open"/>
        <Button mnemonicParsing="false" onAction="#save" text="Save"/>
    </VBox>
</HBox>
